<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
	<session name="TEX1"
				role = "acceptor"
				fix_version="1100"
			     	no_chksum="true"
				active="true"
				heartbeat_interval="10"
				ip="127.0.0.1" port="11002"
				session_log="session_log"
				protocol_log="protocol_log"
				persist="bdb0"
				sender_comp_id="ORDERMATCH"
				reset_sequence_numbers="true"
				tcp_nodelay="false"
			   	tcp_recv_buffer="100663296"
			   	tcp_send_buffer="100663296"
			   	process_model="threaded" />

	<persist name="bdb0"
				type="file"
				dir="./run"
				db="hf_server.db"/>

	<log 		name="session_log"
				type="session"
				filename="|/bin/cat"
				rotation="5"
				flags="timestamp|sequence|compress|thread"/>

	<log 		name="protocol_log"
				type="protocol"
				filename="./run/hf_server_protocol.log"
				rotation="5"
				flags="sequence|append|direction|inbound|outbound"/>
</fix8>
